/* 常用样式
----------------------------------------------------------------------------- */
/* 清理浮动 */
.fn-clear:after {
	content: " ";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden;
	font-size: 0;
}
.fn-clear {
	*zoom: 1; /* for IE6 IE7 */
}

/* 清理浮动类 */
.fn-cl {
	clear: left;
}
.fn-cr {
	clear: right;
}
.fn-cb {
	clear: both;
}

/* 设置内联, 减少浮动带来的 bug */
.fn-fl,.fn-fr {
	display: inline;
}
.fn-fl {
	float: left;
}
.fn-fr {
	float: right;
}

.clear {
	clear: both;
	overflow: hidden;
}

/** 意向岗位弹窗 **/
.gw-select,
.area-select,
.jobs-select {
	display: inline-block;
	width: 218px;
	height: 24px;
	line-height: 24px;
	color: #fff;
	text-indent: 10px;
	background: url(../images/btnw.gif) no-repeat 100% 50%;
}
.area-select {
	float: left;
	width: 100px;
	background-image: url(../images/btn1.gif);
}
.gw-select:hover,
.area-select:hover,
.jobs-select:hover {
	text-decoration: none;
	color: #fff;
}
.data-loading {
	height: 200px;
	background: url(../images/data-loading.gif) no-repeat 50% 50%;
}
#area-sublist .data-loading {
	height: 80px;
}
/* 选中的岗位 */
#gw-selected,
#area-selected {
	clear: both;
	height: auto;
	overflow: hidden;
	*zoom: 1;
}
#gw-selected {
	/* min-height: 28px; */
	margin-top: 5px;
	/* _height: 28px; */
}
#gw-selected .gw-selected-item,
#area-selected .area-selected-item {
	float: left;
	margin: 5px 8px 0 0;
	text-shadow: 1px 1px 0 #eee;
	white-space: nowrap;
	border: 1px solid #DDDDDD;
	background: #efefef;
}
#gw-selected .gw-selected-item .text,
#area-selected .area-selected-item .text {
	float: left;
	height: 21px;
	line-height: 21px;
	padding: 0 9px;
	color: #333;
}
#gw-selected .gw-selected-item .closer,
#area-selected .area-selected-item .closer {
	float: left;
	width: 18px;
	height: 21px;
	line-height: 21px;
	color: #333;
	font-size: 12px;
	font-weight: normal;
	text-align: center;
	text-decoration: none;
	background-color: #D8D8D8;
}
#gw-selected .gw-selected-item .closer:hover,
#area-selected .area-selected-item .closer:hover {
	color: #fff;
	background-color: #FF8119;
}

/* 岗位列表 */
#gw-list a,
#area-list a {
	/* position: relative; */
	width: 180px;
	margin-right: 10px;
	text-decoration: none;
	outline: none;
	text-indent: 4px;
}
#gw-list a:hover,
#area-list a:hover {
	text-decoration: none;
	background-color: #E9E9E9;
}
#gw-list a.cur,
#area-list a.cur {
	color: #fff;
	background-color: #FF8B29;
}
#gw-list a.cur:hover,
#area-list a.cur:hover { /* for 操蛋的 IE6 */
	color: #fff;
	background-color: #FF8B29;
}

/* 搜索 */
.gw-search-wrapper {
	margin: 10px 10px 0;
}
#gw-search {
	display: inline-block;
	width: 148px;
	height: 19px;
	line-height: 19px;
	border: 1px solid #CCCCCC;
	background: #fff url(../images/icon-search.png) no-repeat 5px 50%;
	*display: inline;
	*zoom: 1;
}
#gw-search .search-wd {
	height: 19px;
	line-height: 19px;
	margin: 0 0 0 14px;
	padding: 0 0 0 4px;
	width: 110px;
	border: none;
	vertical-align: top;
	color: #999999;
	background: none;
	margin-left: 6px\9;
}
#gw-search .search-wd:focus {
	color: #444;
}
#gw-search .closer {
	float: none;
	display: inline-block;
	margin: 4px 3px 0 0;
	height: 11px;
	width: 11px;
	overflow: hidden;
	outline: none;
	background: url(../images/icon-closer-search.png) no-repeat;
	margin-right: -3px\9;
}
#gw-search .closer.active {
	background-image: url(../images/icon-closer-active-search.png);
}

/* 错误提示 */
#gw-err,
#area-err {
	display: none;
	line-height: 19px;
	margin: 0 4px;
	color: #f00;
}

/* 岗位次级列表弹窗 */
#gw-sublist,
#area-sublist {
	position: absolute;
	/* wtop: 20px; */
	/* wleft: 20px; */
	z-index: 100;
	padding: 5px;
	width: 380px;
	border: 2px solid #FF8119;
	background: #FFFFE5;
	overflow: hidden;
	display: none;
}
#gw-sublist span,
#area-sublist span {
	color: #333333;
	display: block;
	float: left;
	display: inline;
